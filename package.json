{
  "name": "egis",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "serve": "node scripts/update-build.js && vue-cli-service serve",
    "build": "node scripts/update-build.js && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "@quasar/extras": "^1.0.0",
    "@vue/runtime-dom": "^3.5.13",
    "axios": "^0.24.0",
    "carbone-sdk-js": "^1.1.7",
    "core-js": "^2.6.12",
    "devextreme": "20.1.7",
    "devextreme-vue": "20.1.7",
    "docxtemplater": "^3.21.1",
    "emailjs": "^3.4.0",
    "exceljs": "^4.1.0",
    "file-saver": "^2.0.5",
    "googlemaps": "^1.12.0",
    "html2pdf.js": "^0.10.1",
    "jquery": "^3.7.1",
    "jsbarcode": "^3.11.6",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.13",
    "localbase": "^0.7.5",
    "node-sass": "^4.14.1",
    "pizzip": "^3.0.6",
    "popper.js": "^1.16.1",
    "powerbi-client": "^2.21.1",
    "qrcode": "^1.5.4",
    "qrcode-pix": "5.0.0",
    "quasar": "^1.0.0",
    "react": "^19.2.0",
    "regenerator-runtime": "^0.14.1",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.10",
    "vue-browser-geolocation": "^1.8.0",
    "vue-cryptojs": "^2.1.4",
    "vue-html2pdf": "^1.6.4",
    "vue-router": "^3.0.1",
    "vue-types": "^5.1.1",
    "vue-xlsx": "^0.2.1",
    "vue2-google-maps": "^0.10.7",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "whatwg-fetch": "^3.5.0",
    "xlsx": "^0.14.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-pwa": "^4.5.9",
    "@vue/cli-service": "^3.12.0",
    "babel-eslint": "^10.1.0",
    "cldr-data": "^36.0.0",
    "devextreme-cldr-data": "^1.0.3",
    "devextreme-cli": "1.2.1",
    "devextreme-intl": "^19.1.8",
    "devextreme-themebuilder": "20.1-stable",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "globalize": "^1.6.0",
    "postcss-rtl": "^1.2.3",
    "sass-loader": "^7.1.0",
    "prettier": "^2.8.8",
    "vue-cli-plugin-quasar": "~3.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "vue/no-unused-components": "warn",
      "no-debugger": "off",
      "vue/no-mutating-props": "off",
      "vue/multi-word-component-names": "off",
      "vue/require-default-prop": "off",
      "vue/attribute-hyphenation": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
